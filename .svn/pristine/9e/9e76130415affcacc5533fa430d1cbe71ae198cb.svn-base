        private static string GetHeaderFooter(MemoryStream memoryStream, bool HeaderFooter = true, bool WaterMark = true)
        {
            string OutPutData = "";
            try
            {
                MemoryStream OutSteram = new MemoryStream();
                string CompanyLogo = "";
                string Manufacturer = "";
                iTextSharp.text.Image image = null;
                if (HeaderFooter)
                {
                    DataTable dt = new DataTable();
                    string Err = "";
                    if (MiddleTyre_Mysql.GetSystemDetails(ref dt, ref Err))
                    {
                        if (dt.Rows.Count > 0)
                        {
                            Manufacturer = dt.Rows[0]["Manufacturer"].ToString();
                            CompanyLogo = dt.Rows[0]["Banner"].ToString();
                            string Logo2 = dt.Rows[0]["Logo"].ToString();
                        }
                    }
                    dt.Clear();
                    dt.Dispose();
                    dt = null;
                    CompanyLogo = Regex.Split(CompanyLogo, "base64,")[1];
                    byte[] byteBuffer = Convert.FromBase64String(CompanyLogo);
                    //image = iTextSharp.text.Image.GetInstance(preImage, ImageFormat.Png);
                    image = iTextSharp.text.Image.GetInstance(byteBuffer);
                    //image.ScalePercent(20F);
                    image.ScaleAbsolute(150f, 35f);
                }
                byte[] byteInfo = memoryStream.ToArray();
                var oldFile = new PdfReader(byteInfo);
                //in production, I use MemoryStream
                //I put FileStream here to test the code in console application
                //using (var newFileStream = new FileStream(@"C:\Users\Srinivas\Desktop\Test PDF\tt.pdf", FileMode.Create))
                using (var newFileStream = new MemoryStream())
                {
                    //setup PdfStamper
                    var stamper = new PdfStamper(oldFile, newFileStream);
                    if (image != null)
                    {
                        image.SetAbsolutePosition(30f, stamper.Reader.GetPageSize(1).Height - 50);
                    }
                    LineSeparator line = new LineSeparator(0.5f, 50f, BaseColor.Cyan, Element.ALIGN_LEFT, -5);
                    Phrase phrase = new Phrase();
                    phrase.Add(line);
                    //iterate through the pages in the original file
                    for (var i = 1; i <= oldFile.NumberOfPages; i++)
                    {
                        //get canvas for current page
                        var canvas = stamper.GetOverContent(i);
                        //add image with pre-set position and size
                        if (image != null)
                        {
                            // --------- Add Logo on top
                            canvas.AddImage(image);
                        }
                        if (WaterMark)
                        {
                            iTextSharp.text.Rectangle pageSize = oldFile.GetPageSizeWithRotation(i);
                            PdfContentByte pdfPageContents = stamper.GetUnderContent(i);
                            pdfPageContents.BeginText();
                            BaseFont baseFont = BaseFont.CreateFont(BaseFont.HELVETICA_BOLD, Encoding.ASCII.EncodingName, true);
                            //baseFont = new iTextSharp.text.BaseFont(iTextSharp.text.Font.HELVETICA, 60, iTextSharp.text.Font.NORMAL, iTextSharp.text.Color.LIGHT_GRAY);
                            pdfPageContents.SetFontAndSize(baseFont, 45); // 40 point font
                            //pdfPageContents.SetRGBColorFill(140, 180, 205); // Sets the color of the font, RED in this instance
                            pdfPageContents.SetRgbColorFill(224, 224, 224); // Sets the color of the font, RED in this instance
                            float textAngle = (float)FooTheoryMath.GetHypotenuseAngleInDegreesFrom(pageSize.Height, pageSize.Width);
                            pdfPageContents.ShowTextAligned(PdfContentByte.ALIGN_CENTER, Manufacturer, pageSize.Width / 2, pageSize.Height / 2, textAngle);
                            pdfPageContents.EndText();
                        }
                        //ColumnText.ShowTextAligned(stamper.GetUnderContent(i), Element.ALIGN_CENTER, new Phrase("Phone: +91-9959955506, +91-40-6555-5708", FooterFont), stamper.Reader.GetPageSize(i).Width / 2, 30f, 0);
                        //if (Website != "")
                        //    ColumnText.ShowTextAligned(stamper.GetUnderContent(i), Element.ALIGN_CENTER, new Phrase("Email: " + CompanyMailId + "; Website: " + Website, FooterFont), stamper.Reader.GetPageSize(i).Width / 2, 15f, 0);
                        //int moveval = 25;
                        //int k = 0;
                        //int w = (int)stamper.Reader.GetPageSize(i).Width;
                        if (HeaderFooter)
                        {
                            ColumnText.ShowTextAligned(stamper.GetUnderContent(i), Element.ALIGN_LEFT, phrase, 0f, stamper.Reader.GetPageSize(i).Height - 50, 0);
                        }
                        //ColumnText.ShowTextAligned(stamper.GetUnderContent(i), Element.ALIGN_LEFT, phrase, 0f, stamper.Reader.GetPageSize(i).Height - 75, 0);
                    }
                    stamper.Close();
                    newFileStream.Position = 0;
                    newFileStream.CopyTo(OutSteram);
                    byteInfo = OutSteram.ToArray();
                    OutPutData = "data:application/pdf;base64," + Convert.ToBase64String(byteInfo);
                }
            }
            catch (Exception ex)
            {
            }
            return OutPutData;
        }

