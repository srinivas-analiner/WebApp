                    DirectoryInfo info = new DirectoryInfo(Environment.CurrentDirectory + "//wwwroot//public//Logs");
                    FileInfo[] files = info.GetFiles("*.txt").OrderByDescending(p => p.CreationTime).ToArray().Take(2).ToArray();
