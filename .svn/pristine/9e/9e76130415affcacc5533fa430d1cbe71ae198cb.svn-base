                    cell.DisableBorderSide(iTextSharp.text.Rectangle.BOTTOM_BORDER);
                    cell.DisableBorderSide(iTextSharp.text.Rectangle.RIGHT_BORDER);
                    cell.DisableBorderSide(iTextSharp.text.Rectangle.LEFT_BORDER);
                }
                table.AddCell(cell);
                pdfDoc.Add(table);
            }
            catch (Exception)
            {
            }
        }

