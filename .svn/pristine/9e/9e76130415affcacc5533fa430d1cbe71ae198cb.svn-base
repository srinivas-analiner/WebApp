        private static string IconToBase64(Icon icon)
        {
            string Base64 = "";
            try
            {
                using (var ms = new MemoryStream())
                {
                    icon.ToBitmap().Save(ms, ImageFormat.Png);
                    Base64 = "data:image/x-icon;base64," + Convert.ToBase64String(ms.ToArray());
                }
            }
            catch (Exception)
            {
            }
            return Base64;
        }

