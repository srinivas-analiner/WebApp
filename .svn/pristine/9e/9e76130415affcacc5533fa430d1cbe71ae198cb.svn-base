        [HttpPost]
        public JsonResult GetRecordingFiles([FromBody] JObject Data)
        {
            var dt = new DataTable
            {
                Columns = { { "FileNames", typeof(string) } }
            };
            try
            {
                bool GetAllFiles = true;
                string Type = Common.GetJArrayValue(Data, "Type");
                string Filter = Common.GetJArrayValue(Data, "Filter");
                bool sort = Convert.ToBoolean(Common.GetJArrayValue(Data, "sort"));
                switch (Filter)
                {
                    case "Both":
                        GetAllFiles = true;
                        break;
                    case "Thermal":
                        GetAllFiles = false;
                        break;
                    case "Day":
                        GetAllFiles = false;
                        break;
                }
                string FileFormat = "*.mp4";
                DirectoryInfo info = null;
                switch (Type)
                {
                    case "Videos":
                        info = new DirectoryInfo(Environment.CurrentDirectory + "//wwwroot//public//video");
                        FileFormat = "*.mp4";
                        break;
                    case "Snaps":
                        info = new DirectoryInfo(Environment.CurrentDirectory + "//wwwroot//public//snapshot");
                        FileFormat = "*.jpg";
                        break;
                    default: break;
                }

                FileInfo[] files = null;// info.GetFiles("*.mp4").OrderByDescending(p => p.CreationTime).ToArray();
                if (sort)
                {
                    files = info.GetFiles(FileFormat).OrderByDescending(p => p.CreationTime).ToArray();
                }
                else
                {
                    files = info.GetFiles(FileFormat).OrderBy(p => p.CreationTime).ToArray();
                }
                foreach (FileInfo FI in files)
                {
                    if (GetAllFiles)
                        dt.Rows.Add(Path.GetFileNameWithoutExtension(FI.Name));
                    else
                    {
                        if (FI.Name.Contains(Filter, StringComparison.CurrentCultureIgnoreCase))
                        {
                            dt.Rows.Add(Path.GetFileNameWithoutExtension(FI.Name));
                        }
                    }
                }
                files = null;
                info = null;
            }
            catch (Exception ex)
            {
            }
            List<string> s = dt.AsEnumerable().Select(x => x[0].ToString()).ToList();
            return Json(s);
        }

