        private bool GetReport(ref string URL)
        {
            bool Status = false;
            try
            {
                string ErrorMessage = "";
                DataTable dt2 = new DataTable();
                int loop = 5000;
                string[] Types_Array = new string[loop];
                string[] SubTypes_Array = new string[loop];
                string[] Remarks_Array = new string[loop];
                string[] Status_Array = new string[loop];
                MiddleTyre_Mysql.GetBuiltInTestTypes(ref dt2, ref ErrorMessage);
                int ck = 0;
                foreach (DataRow dr in dt2.Rows)
                {
                    string TypeTemp = dr["Type"].ToString();
                    DataTable dtResults = new DataTable();
                    string Err = "";
                    MiddleTyre_Mysql.GetBuiltInTestResult(TypeTemp, ref dtResults, ref Err);
                    if (dtResults.Rows.Count > 0)
                    {
                        foreach (DataRow dr2 in dtResults.Rows)
                        {
                            Types_Array[ck] += TypeTemp + ";";
                            SubTypes_Array[ck] += dr2["SubType"].ToString() + ";";
                            Remarks_Array[ck] += dr2["Remarks"].ToString() + ";";
                            Status_Array[ck] += dr2["Status"].ToString() + ";";
                        }
                        ck++;
                    }

                }

                Types_Array = Types_Array.Where(x => x != null).ToArray();
                for (int i = 0; i < Types_Array.Length; i++)
                {
                    var Types_Array2 = Types_Array[i].Split(';').Distinct().ToArray();
                    Types_Array2 = Types_Array2.Where(x => !string.IsNullOrWhiteSpace(x)).ToArray();
                    Types_Array[i] = Types_Array2[0];
                }
                SubTypes_Array = SubTypes_Array.Where(x => x != null).ToArray();
                Remarks_Array = Remarks_Array.Where(x => x != null).ToArray();
                Status_Array = Status_Array.Where(x => x != null).ToArray();

                DataTable dtFinal = new DataTable();
                DataColumn dcType = new DataColumn("Type", typeof(string));
                DataColumn dcSunTypes = new DataColumn("SubType", typeof(string));
                DataColumn dcRemarks = new DataColumn("Remarks", typeof(string));
                DataColumn dcStatus = new DataColumn("Status", typeof(string));

                dtFinal.Columns.Add(dcType);
                dtFinal.Columns.Add(dcSunTypes);
                dtFinal.Columns.Add(dcRemarks);
                dtFinal.Columns.Add(dcStatus);
                for (int k = 0; k < Types_Array.Length; k++)
                {
                    DataRow dr = dtFinal.NewRow();
                    dr[0] = Types_Array[k];
                    dr[1] = SubTypes_Array[k];
                    dr[2] = Remarks_Array[k];
                    dr[3] = Status_Array[k];
                    dtFinal.Rows.Add(dr);
                }
                Status = GenerateBITPDF(dtFinal, "Built In Test Report", ref URL);
                dt2.Clear();
                dt2.Dispose();
                dt2 = null;
            }
            catch (Exception ex)
            {
            }
            return Status;
        }

