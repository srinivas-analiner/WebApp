        public JsonResult GetTrackingParameters([FromBody] JObject Data)
        {
            string Result = "Failed";
            int ObjWidth = 30;
            string Mode = "";
            int Maxmisses = 40;
            int Near_Value = 50;
            bool Acquasition_Assist = false;
            bool Intelligent_Assist = false;
            int ObjHeight = 30;
            try
            {
                string VideoInput = "Device0";
                VideoInput = Common.GetDeviceName(Common.GetJArrayValue(Data, "VideoInput"));
                bool TrackingFlag = Convert.ToBoolean(Common.GetJArrayValue(Data, "TrackingFlag"));
                string Type = "GetTracking_Parameter";
                string DataIn = Common.GetCommand("Tracking", Type, VideoInput);
                string Err = "";
                if (TrackingFlag)
                {
                    DataIn += "01";
                }
                else
                {
                    DataIn += "02";
                }
                UdpClient client = new UdpClient();
                if (UDP.Connect(IpAddress, port, ref client, ref Err))
                {
                    Err = "";
                    if (UDP.ATC_TransmitData(client, DataIn, ref Err))
                    {
                        string OutData = null;
                        if (UDP.ATC_ReceiveData(client, ref OutData, ref Err))
                        {
                            if (OutData.Substring(0, 8) == DataIn.Substring(0, 6) + "01")
                            {
                                if (TrackingFlag)
                                {
                                    string DataOut = OutData.Substring(8, 16);
                                    ObjWidth = int.Parse(DataOut.Substring(0, 2), System.Globalization.NumberStyles.HexNumber);
                                   int Mode_Temp = int.Parse(DataOut.Substring(2, 2), System.Globalization.NumberStyles.HexNumber);
                                    Maxmisses = int.Parse(DataOut.Substring(4, 2), System.Globalization.NumberStyles.HexNumber);
                                    Near_Value = int.Parse(DataOut.Substring(8, 2) + DataOut.Substring(6, 2), System.Globalization.NumberStyles.HexNumber);
                                    if (int.Parse(DataOut.Substring(10, 2), System.Globalization.NumberStyles.HexNumber) == 1)
                                    {
                                        Acquasition_Assist = true;
                                    }
                                    if (int.Parse(DataOut.Substring(12, 2), System.Globalization.NumberStyles.HexNumber) == 1)
                                    {
                                        Intelligent_Assist = true;
                                    }
                                    ObjHeight = int.Parse(DataOut.Substring(14, 2), System.Globalization.NumberStyles.HexNumber);
                                    switch (Mode_Temp)
                                    {
                                        case 3:
                                            Mode = "Person";
                                            break;
                                        case 2:
                                            Mode = "Vehicle";
                                            break;
                                        case 8:
                                            Mode = "Drone";
                                            break;
                                        default:
                                            Mode = "Person";
                                            break;
                                    }
                                }
                                Result = "Sucess";
                            }
                            else
                            {
                                Result = OutData;
                            }
                        }
                        else
                        {
                            Result = Err;
                        }
                    }
                    else
                    {
                        Result = Err;
                    }
                    UDP.DisConnect(client, ref Err);
                }
                else
                {
                    Result = Err;
                }
            }
            catch (Exception ex)
            {
                Result = ex.Message;
            }
            JsonSerializerSettings ss = new JsonSerializerSettings();
            var jsonResult = Json(new
            {
                Result = Result,
                ObjWidth = ObjWidth,
                Mode = Mode,
                Maxmisses = Maxmisses,
                Near_Value = Near_Value,
                Acquasition_Assist = Acquasition_Assist,
                Intelligent_Assist = Intelligent_Assist,
                ObjHeight = ObjHeight
            }, ss);
            return jsonResult;
        }


