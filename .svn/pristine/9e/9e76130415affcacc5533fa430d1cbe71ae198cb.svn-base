                        UdpClient client = new UdpClient();
                        if (UDP.Connect(IpAddress, port, ref client, ref Err))
                        {
                            Err = "";
                            if (UDP.ATC_TransmitData(client, DataIn, ref Err))
                            {
                                string DataOut = null;
                                if (UDP.ATC_ReceiveData(client, ref DataOut, ref Err))
                                {
                                    if (DataOut.Length >= 6)
                                    {
                                        if (DataOut == DataIn + "01")
                                        {
                                            Result = "Sucess";
                                            if (DataIn == "AE0101" || DataIn == "AE0201")
                                            {
                                                HttpContext.Session.SetString("PoolCheckCount", "1");
                                                HttpContext.Session.SetString("PreviousPath", "");
                                                HttpContext.Session.SetString("PreviousPathCount", "1");
