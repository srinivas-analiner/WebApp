        internal static string TrackingCRCCalculate(string Value)
        {
            try
            {
                string Value_Crc = Value.Substring(6, Value.Length - 6);
                var bytes = HexToBytes(Value_Crc);
                ushort initialValue = 1;
                ushort crc = initialValue;
                for (int i = 0; i < bytes.Length; ++i)
                {
                    crc = (ushort)crc8_Table[crc ^ bytes[i]];// (ushort)((crc << 8) ^ crc8_Table[((crc >> 8) ^ (0xFF & bytes[i]))]);
                }
                Value = Value + crc.ToString("x2");
            }
            catch (Exception ex)
            {
            }
            return Value.ToUpper();
        }

