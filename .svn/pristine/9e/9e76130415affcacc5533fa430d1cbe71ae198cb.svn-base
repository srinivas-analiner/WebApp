    internal static class TCP
    {
        //internal static TcpClient tcpclnt = new TcpClient();
        //internal static Stream stm;

        static int ReceiveTimeout = 500;
        static int SendTimeout = 500;

        internal static bool Connect(string Ip, int Port, ref TcpClient tcpclnt, ref Stream stm, ref string Error)
        {
            try
            {
                tcpclnt = new TcpClient();
                tcpclnt.ReceiveTimeout = ReceiveTimeout;
                tcpclnt.SendTimeout = SendTimeout;
                tcpclnt.Connect(Ip, Port);
                stm = tcpclnt.GetStream();
            }
            catch (Exception ex)
            {
                Error = ex.Message;
                return false;
            }
            return true;
        }

        internal static bool ATC_TransmitData(Stream stm, string Data, ref string Error)
        {
            try
            {
                byte[] ba = Common.HexToBytes(Data);
                stm.Write(ba, 0, ba.Length);
                stm.Flush();
            }
            catch (Exception ex)
            {
                Error = ex.Message;
                return false;
            }
            return true;
        }

        internal static bool ATC_ReceiveData(Stream stm, int DataLength, ref string Data, ref string Error)
        {
            try
            {
                Thread.Sleep(300);
                int size = DataLength / 2;
                var bytes = new byte[size];
                var total = 0;
                try
                {
                    do
                    {
                        var read = stm.Read(bytes, total, size - total);
                        Debug.WriteLine("Client recieved {0} bytes", total);
                        if (read == 0)
                        {
                            //If it gets here and you received 0 bytes it means that the Socket has Disconnected gracefully (without throwing exception) so you will need to handle that here
                        }
                        total += read;
                        //If you have sent 1024 bytes and Receive only 512 then it wil continue to recieve in the correct index thus when total is equal to 1024 you will have recieved all the bytes
                    } while (total != size);
                }
                catch (Exception ex)
                {
                }
                Data = Common.ExtractDataFromBuffer(DataLength, bytes);
                //string dd= System.Text.Encoding.ASCII.GetString(bytes);
                //int l = 0;
                ////stm.Flush();
                //Data = dd;
            }
            catch (Exception ex)
            {
                Error = ex.Message;
                return false;
            }
            return true;
        }

        internal static bool ATC_ReceiveData(Stream stm, int DataLength, ref byte[] Data, ref string Error)
        {
            try
            {
                Thread.Sleep(50);
                int size = DataLength;
                var bytes = new byte[size];
                var total = 0;
                for (int ii = 0; ii < 15; ii++)
                {
                    try
                    {
                        do
                        {
                            var read = stm.Read(bytes, total, size - total);
                            Debug.WriteLine("Client recieved {0} bytes", total);
                            if (read == 0)
                            {
                                //If it gets here and you received 0 bytes it means that the Socket has Disconnected gracefully (without throwing exception) so you will need to handle that here
                            }
                            total += read;
                            //If you have sent 1024 bytes and Receive only 512 then it wil continue to recieve in the correct index thus when total is equal to 1024 you will have recieved all the bytes
                        } while (total != size);
                        //break;
                    }
                    catch (Exception)
                    {
                    }

                    if (total > 0)
                    {
                        break;
                    }
                }
                Data = new byte[total];
                // Data = bytes;

                Buffer.BlockCopy(bytes, 0, Data, 0, total);

            }
            catch (Exception ex)
            {
                Error = ex.Message;
                return false;
            }
            return true;
        }

