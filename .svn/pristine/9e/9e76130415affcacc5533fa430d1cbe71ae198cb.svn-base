        internal static byte[] HexToBytes(string InputData)
        {
            byte[] result = new byte[InputData.Length / 2];
            for (int i = 0; i < result.Length; i++)
            {
                result[i] = Convert.ToByte(InputData.Substring(2 * i, 2), 16);
            }
            return result;
        }

        internal static string ExtractDataFromBuffer(int DataLength, byte[] UARTInputData, bool RemoveZerosatEnd = true, int RemoveZeroLocation = 30)
        {
            string Data = "";
            {
                try
                {
                    Data = string.Join("", UARTInputData.Select(b => Convert.ToString(b, 16).ToUpper().PadLeft(2, '0')));
                    Data = Data.Substring(0, DataLength);
                    if (RemoveZerosatEnd)
                    {
                        string Error = "00";
                        try
                        {
                            int ErrorValueStart = Data.IndexOf(Error.PadLeft(RemoveZeroLocation, '0'));
                            if (ErrorValueStart > 0)
                            {
                                Data = Data.Substring(0, ErrorValueStart);
                            }
                        }
                        catch (Exception ex)
                        {
                        }
                    }
                }
                catch (Exception ex)
                {
                    Data = "";
                }
            }
            return Data;
        }

