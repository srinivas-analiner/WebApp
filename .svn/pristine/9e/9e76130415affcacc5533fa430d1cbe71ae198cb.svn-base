        internal static bool ATC_ReceiveData(UdpClient Client, ref string Data, ref string Error)
        {

            bool ss = false;
            try
            {
                IPEndPoint listenPort = (IPEndPoint)Client.Client.RemoteEndPoint;
                for (int i = 0; i < 5; i++)
                {
                    try
                    {
                        Thread.Sleep(25);
                        byte[] bytes = Client.Receive(ref listenPort);
                        Data = Common.ExtractDataFromBuffer(bytes.Length * 2, bytes);
                        Error = "";
                        ss = true;
                        break;
                    }
                    catch (Exception ex)
                    {
                        ss = false;
                        if (ex.Message == "A connection attempt failed because the connected party did not properly respond after a period of time, or established connection failed because connected host has failed to respond.")
                        {
                            Error = "Failed !";
                        }
                        else
                            Error = ex.Message;
                        Thread.Sleep(50);
                    }
                }
            }
            catch (Exception ex)
            {
                ErrCheck(ex.Message);
                ss = false;
            }
            return ss;
        }

