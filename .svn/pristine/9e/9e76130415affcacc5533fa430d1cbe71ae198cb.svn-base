        private static void AddCell(Document pdfDoc, PdfPTable table, PdfPCell cell,
                                Phrase DisplayText, float MinimumHeight = 5, int HorizontalAlignment = 1, bool DisableBorderSide = true)
        {
            try
            {
                cell = new PdfPCell(DisplayText);
