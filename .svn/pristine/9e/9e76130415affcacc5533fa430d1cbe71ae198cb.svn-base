        public JsonResult getLineChartData()
        {
            List<object> iData = new List<object>();
            List<int> labels = new List<int>();
            int Min = 0;
            int Max = 0;
            try
            {
                for (int i = 1; i <= 50; i++)
                {
                    labels.Add(i);
                }
                iData.Add(labels);
                string query_DataSet_1 = "";
                DataTable dtDataItemsSets_1 = new DataTable();
                //MiddleTyre_Mysql.GetTemperatureDetails("Processor", "Device0", ref dtDataItemsSets_1, ref query_DataSet_1);
                MiddleTyre_Mysql.GetTrackerTelemetryDetails("Azimuth", ref dtDataItemsSets_1, ref query_DataSet_1);
                List<double> lst_dataItem_1 = new List<double>();
                foreach (DataRow dr in dtDataItemsSets_1.Rows)
                {
                    lst_dataItem_1.Add((Convert.ToDouble(Convert.ToDouble(dr["Azimuth_Error"].ToString()).ToString("0.0"))));
                }
                lst_dataItem_1.Reverse();
                iData.Add(lst_dataItem_1);
                string query_DataSet_2 = "";
                DataTable dtDataItemsSets_2 = new DataTable();
                //MiddleTyre_Mysql.GetTemperatureDetails("FPA", "Device0", ref dtDataItemsSets_2, ref query_DataSet_2);
                MiddleTyre_Mysql.GetTrackerTelemetryDetails("Elevation", ref dtDataItemsSets_2, ref query_DataSet_2);
                List<double> lst_dataItem_2 = new List<double>();
                foreach (DataRow dr in dtDataItemsSets_2.Rows)
                {
                    lst_dataItem_2.Add(Convert.ToDouble(Convert.ToDouble(dr["Elevation_Error"].ToString()).ToString("0.0")));
                }
                lst_dataItem_2.Reverse();
                iData.Add(lst_dataItem_2);
                double Mint = 0;
                double Maxt = 0;
                Mint = lst_dataItem_1.Min();
                Maxt = lst_dataItem_1.Max();
                if (Mint > lst_dataItem_2.Min())
                {
                    Mint = lst_dataItem_2.Min();
                }
                if (Maxt < lst_dataItem_2.Max())
                {
                    Maxt = lst_dataItem_2.Max();
                }
                Mint = Mint - (Mint * 0.2);
                Maxt = Maxt + (Mint * 0.2);
                Min = (int)Mint;
                Max = (int)Math.Ceiling(Maxt);
                dtDataItemsSets_2.Clear();
                dtDataItemsSets_2.Dispose();
                dtDataItemsSets_2 = null;
                dtDataItemsSets_1.Clear();
                dtDataItemsSets_1.Dispose();
                dtDataItemsSets_1 = null;
            }
            catch (Exception ex)
            {
            }
            JsonSerializerSettings ss = new JsonSerializerSettings();
            var jsonResult = Json(new { iData = iData, Min = Min, Max = Max }, ss);
            //jsonResult.SerializerSettings.MaxDepth= int.MaxValue;
            return jsonResult;
        }

