        {
            double percentComplete = 0;
            try
            {
                int TotalPackets = 1;
                int PacketSend = 0;
                for (int i = 0; i < 10; i++)
                {
                    string Err = "";
                    DataTable dt = new DataTable();
                    if (MiddleTyre_Mysql.GetCamFirmwareUpdateDetails(ref dt, ref Err))
                    {
                        if (dt.Rows.Count > 0)
                        {
                            PacketSend = Convert.ToInt32(dt.Rows[0]["SendPackets"].ToString());
                            TotalPackets = Convert.ToInt32(dt.Rows[0]["TotalPackets"].ToString());
                        }
                        dt.Clear();
                        dt.Dispose();
                        dt = null;
                        break;
                    }
                }
                percentComplete = ((PacketSend * 100.0) / TotalPackets);
                if (percentComplete > 100)
                {
                    percentComplete = 100;
                }
            }
            catch (Exception)
