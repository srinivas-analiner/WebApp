        [HttpPost]
        public JsonResult SetMotionRuleDetails([FromBody] JObject Data)
        {
            string Result = "Failed to Create Rule";
            int RuleNumber = Convert.ToInt32(Common.GetJArrayValue(Data, "MotionRule"));
            string Points = Common.GetJArrayValue(Data, "Points");
            try
            {
                DataTable dtData = HttpContext.Session.GetObjectFromJson<DataTable>("MotionRulesData");
                if (dtData == null)
                {
                    dtData = new DataTable();
                    dtData.Clear();
                    dtData.Columns.Add("MotionRule");
                    dtData.Columns.Add("Points");
                    DataRow _row = dtData.NewRow();
                    _row["MotionRule"] = RuleNumber;
                    _row["Points"] = Points;
                    dtData.Rows.Add(_row);
                    Result = "Sucess";
                }
                else
                {
                    bool RuleExist = false;
                    foreach (DataRow dr in dtData.Rows)
                    {
                        if (Convert.ToInt32(dr["MotionRule"].ToString()) == RuleNumber)
                        {
                            dr["Points"] = Points;
                            RuleExist = true;
                            break;
                        }
                    }
                    if (!RuleExist)
                    {
                        DataRow _row = dtData.NewRow();
                        _row["MotionRule"] = RuleNumber;
                        _row["Points"] = Points;
                        dtData.Rows.Add(_row);
                    }
                    Result = "Sucess";
                }
                HttpContext.Session.SetObjectAsJson("MotionRulesData", dtData);
            }
            catch (Exception ex)
            {
                Result = ex.Message;
            }
            JsonSerializerSettings ss = new JsonSerializerSettings();
            var jsonResult = Json(new { Result = Result }, ss);
            //jsonResult.SerializerSettings.MaxDepth= int.MaxValue;
            return jsonResult;
        }

